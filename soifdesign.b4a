Sub ApplyColor(color As Int)
	Dim rgba As Map = ExtractRGBA(color)

	colorshow1.Color = color

	AlphaSeekBar.Value = rgba.Get("alpha")
	RedSeekBar.Value = rgba.Get("red")
	GreenSeekBar.Value = rgba.Get("green")
	BlueSeekBar.Value = rgba.Get("blue")
End Sub

Sub ExtractRGBA(color As Int) As Map
	Dim rgba As Map
	rgba.Initialize
	rgba.Put("alpha", Bit.UnsignedShiftRight(Bit.And(color, 0xFF000000), 24))
	rgba.Put("red", Bit.UnsignedShiftRight(Bit.And(color, 0x00FF0000), 16))
	rgba.Put("green", Bit.UnsignedShiftRight(Bit.And(color, 0x0000FF00), 8))
	rgba.Put("blue", Bit.And(color, 0x000000FF))
	Return rgba
End Sub

