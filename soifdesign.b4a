	in.Initialize(in.ACTION_VIEW, "")
	Provider.SetFileUriAsIntentData(in, FileNames)

	in.SetType("application/pdf")

	Dim ctxt As JavaObject
	ctxt.InitializeContext
	Dim pm As JavaObject = ctxt.RunMethod("getPackageManager", Null)
	Dim intentList As List = pm.RunMethod("queryIntentActivities", Array(in, 0))
	If WebView1.Visible=True And edite=False Then  slidemeno="savepdf"
	If intentList.Size > -1 Then
		
		Try
			StartActivity(in)
		Catch
			ToastMessageShow(xn(261), False)'"No application available to view PDF files."
		End Try
'	Else
'		ToastMessageShow(xn(261), True)'"No application available to view PDF files."
	End If
