	If sensA=True   Then
		Dim movexx ,moveyy As Int
		movexx=Round2 ( movex,3)
		moveyy=Round2( movey,3)
		yinsf(2)=movexx
		yinsf(3)=moveyy
		If accmulti=0 Then accmulti=1
		yins(2)=(accadd+(movex*accmulti))
		yins(3)=(accadd+(movey*accmulti))

	End If
