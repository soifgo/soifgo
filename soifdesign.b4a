Sub htmlsend(num As String)
	Log(num)
	Dim s As String = "" & num  
	Dim sep1 As String = ";"
	Dim firstIndex As Int = s.IndexOf(sep1)
	Dim secondIndex As Int = s.IndexOf2(sep1, firstIndex + 1)

	If firstIndex > -1 And secondIndex > -1 Then
		Dim part1 As String = s.SubString2(0, firstIndex).Trim
		Dim part2 As String = s.SubString2(firstIndex + sep1.Length, secondIndex).Trim
		Dim part3 As String = s.SubString(secondIndex + sep1.Length).Trim

		'Log("🔸 بخش اول: " & part1)
		'Log("🔹 بخش دوم: " & part2)
		'Log("🔹 بخش سوم: " & part3)

		Dim parts() As String = Regex.Split(" ", part1)
		If IsUrl(parts(0)) Then
			ser(part1,part2,part3)
		Else
			Select Case part1
				Case "blu" , "Blu" , "BLU"
					blu(part2,part3)
				Case "sms" , "Sms" , "SMS"
					sms(part2,part3)
			End Select
		End If
	'Else
		'Log("⚠️ جداکننده یافت نشد")
	End If

End Sub
Sub blu(part2 As String,part3 As String)
	If connected Then

		Select  part2
			Case "crlf" , "CRLF","Crlf"
				TextWriter1.Write(part3&Chr(13)&Chr(10))
				TextWriter1.Flush
				msg.Text=part3
			Case "cr" , "CR","Cr"
					TextWriter1.Write(part3&Chr(13))
				TextWriter1.Flush
				msg.Text=part3
			Case "lf" , "LF","Lf"
					TextWriter1.Write(part3&Chr(10))
				TextWriter1.Flush
				msg.Text=part3
				Case "stx/etx" , "STX/ETX", "Stx/etx"
					TextWriter1.Write(Chr(2)&part3&Chr(3))
				TextWriter1.Flush
				msg.Text=part3
				Case "null" , "NULL","Null"
					TextWriter1.Write(part3&Chr(0))
				TextWriter1.Flush
				msg.Text=part3
				Case "non" , "NON","Non"
					TextWriter1.Write(part3)
				TextWriter1.Flush
				msg.Text=part3
		End Select
	Else
		
			ToastMessageShow(xn(24),False)'"Disconnect Bluetooth  !"
			error
	End If
	
End Sub

