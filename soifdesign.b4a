Sub connect_hmi_Click
	Dim bloo As Boolean
	''''''''''''''''''''''''''''''''check bluetooth permision
	Dim SdkVersion As Int = device.SdkVersion
	If SdkVersion < 31 And SdkVersion > 21 Then
		For Each Permission As String In Array("android.permission.BLUETOOTH", "android.permission.BLUETOOTH_ADMIN")
			rp.CheckAndRequest(Permission)
			' منتظر نتیجه درخواست
			Wait For Activity_PermissionResult(Permission As String, Result As Boolean)
			If Result Then
				Log(Permission & " permission granted.")
				' ادامه فرآیند اگر مجوز داده شد
				bloo=True
			Else
				Log(Permission & " permission denied.")
				' پیام به کاربر در صورت رد شدن مجوز
				
				ToastMessageShow(xn(12) & Permission, False)'"Permission denied: "
				bloo=False
				Exit
			End If
		Next

	End If

	

	If ph.SdkVersion >= 31 Then
		For Each Permission As String In Array("android.permission.BLUETOOTH_SCAN", "android.permission.BLUETOOTH_CONNECT", _
        "android.permission.BLUETOOTH_ADVERTISE", "android.permission.ACCESS_FINE_LOCATION")
			rp.CheckAndRequest(Permission)
			' منتظر نتیجه برای هر مجوز
			Wait For Activity_PermissionResult(Permission As String, Result As Boolean)
			If Result Then
				Log(Permission & " permission granted.")
				' اگر مجوز داده شد، ادامه دهید
				bloo=True
			Else
				Log(Permission & " permission denied.")
				' اگر مجوز رد شد، اقدام مناسب انجام دهید
				ToastMessageShow(xn(12) & Permission, False)'"Permission denied: "
				bloo=False
				Exit
			End If
		Next
	End If

	
	

	
	
