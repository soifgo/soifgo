Sub index1w
	
	Wait For InputList_Result (index As Int)
		
	''''''''''''''''''''''''''desin1
	'"CANCEL","delay","LF RF","Send lines is  Auto","Check in ON","Check Next ON","on","LOOP ON"))
	Select Case index
		Case 0
			jobsend=False
			outer.TextColor=Colors.Magenta
			jbsc=0
			jbs=0
			jbt=0
		Case 1
			If noteout(adkeye)=False And waitnote(adkeye)=False  Then
			xscrolvis
			xscrol.BringToFront
			Butsellect="delay"
				ime.HideKeyboard
				Else
				outer_LongClick
           End If
			
		Case 2
			Dim options2 As List = Array("CR+LF","CR","LF","STX/ETX","NULL","NON")
		
			InputListAsync(options2, xn(136), noteformat(adkeye), True)'"Select Serial Send Format"
			Wait For InputList_Result (Index3 As Int)
			Select  Index3
				Case 0
					noteformat(adkeye)=0
					writ
				Case 1
					noteformat(adkeye)=1
					writ
				Case 2
					noteformat(adkeye)=2
					writ
				Case 3
					noteformat(adkeye)=3
					writ
				Case 4
					noteformat(adkeye)=4
					writ
				Case 5
					noteformat(adkeye)=5
					writ
			End Select
		Case 3
			noteout(adkeye)=Not(noteout(adkeye))
			If noteout(adkeye)=True Then
				noteoutloop(adkeye)=False
				checkinnote(adkeye)=False
				checknotenext(adkeye)=False
				waitnote(adkeye)=False
				Else
					notesend.Visible=False
			End If
			

			writ
		Case 4
			checkinnote(adkeye)=Not(checkinnote(adkeye))
			If checkinnote(adkeye)=True Then 	ToastMessageShow(xn(284),False)'"Submissions must be returned"
			writ
		Case 5
			checknotenext(adkeye)=Not(checknotenext(adkeye))
			If checknotenext(adkeye)=True Then waitnote(adkeye)=False

			If checknotenext(adkeye)=True Then 	ToastMessageShow(xn(285),False)'"To continue, you need to send the NEXT or next word"
			writ
		Case 6
			waitnote(adkeye)=Not(waitnote(adkeye))
			If waitnote(adkeye)=True Then checknotenext(adkeye)=False
			If waitnote(adkeye)=True Then 	ToastMessageShow(xn(286),False)'"A timer will be considered after each line instruction"
		Case 7
			If noteout(adkeye)=False Then
				noteoutloop(adkeye)=Not(noteoutloop(adkeye))
			End If
			writ
	End Select
	If index>=2 Then index1z
End Sub

