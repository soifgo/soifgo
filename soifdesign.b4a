Sub takecolorseek
	Dim red,green,blue As String
	red=redvalue1.Text
	green=greenvalue1.Text
	blue=bluevalue1.Text
	If redvalue1.Text<10 Then red="00"&redvalue1.Text
	If redvalue1.Text>10 And redvalue1.Text<100  Then red="0"&redvalue1.Text
	If greenvalue1.Text<10 Then green="00"&greenvalue1.Text
	If greenvalue1.Text>10 And greenvalue1.Text<100  Then green="0"&greenvalue1.Text
	If bluevalue1.Text<10 Then blue="00"&bluevalue1.Text
	If bluevalue1.Text>10 And bluevalue1.Text<100  Then blue="0"&bluevalue1.Text
	msg.Text=red&green&blue
End Sub

Sub ApplyStandardHTMLColors(rs As String)
	txtcolor.Initialize(rs)
    
	If txtcolor = Null Or rs.Length = 0 Then Return

	' ===== 1. رنگ‌بندی DOCTYPE =====
	Dim doctypePattern As String = "<!DOCTYPE.*?>"
	Dim doctypeMatcher As Matcher = Regex.Matcher(doctypePattern, rs)
	Do While doctypeMatcher.Find
		txtcolor.Color(0xFF91D918, doctypeMatcher.GetStart(0), doctypeMatcher.GetEnd(0)) ' طلایی
	Loop

	' ===== 2. رنگ‌بندی تگ‌های HTML =====
	Dim tagPattern As String = "<(/?\w+)(.*?)>"
	Dim tagMatcher As Matcher = Regex.Matcher(tagPattern, rs)
	Do While tagMatcher.Find
		Dim tagStart As Int = tagMatcher.GetStart(0)
		Dim tagEnd As Int = tagMatcher.GetEnd(0)
        
		' رنگ پرانتزهای `<` و `>`
		txtcolor.Color(0xFF439DEC, tagStart, tagStart + 1) ' فیروزه‌ای
		txtcolor.Color(0xFF439DEC, tagEnd - 1, tagEnd) ' فیروزه‌ای
        
		' رنگ نام تگ (قرمز)
		txtcolor.Color(0xFFEC4643, tagMatcher.GetStart(1), tagMatcher.GetEnd(1))

		' ===== 3. رنگ‌بندی ویژگی‌ها و مقادیر =====
		Dim attrText As String = tagMatcher.Group(2)
		If attrText.Length > 0 Then
			Dim attrPattern As String = "(\S+)=(""|')(.*?)\2"
			Dim attrMatcher As Matcher = Regex.Matcher(attrPattern, attrText)
			Do While attrMatcher.Find
				' رنگ نام ویژگی (آبی)
				Dim attrStart As Int = tagMatcher.GetStart(2) + attrMatcher.GetStart(1)
				Dim attrEnd As Int = tagMatcher.GetStart(2) + attrMatcher.GetEnd(1)
				txtcolor.Color(0xFF0E1B5B, attrStart, attrEnd)

				' رنگ مقدار ویژگی (سبز)
				Dim valueStart As Int = tagMatcher.GetStart(2) + attrMatcher.GetStart(3)
				Dim valueEnd As Int = tagMatcher.GetStart(2) + attrMatcher.GetEnd(3)
				txtcolor.Color(0xFFA06300, valueStart, valueEnd)

				' رنگ‌بندی اعداد و مقادیر خاص (بنفش)
				Dim value As String = attrMatcher.Group(3)
				If IsNumber1(value) Or value.StartsWith("#") Or value.StartsWith("rgb(") Then
					txtcolor.Color(0xFF6A5ACD, valueStart, valueEnd)
				End If
			Loop
		End If
	Loop

	' ===== 4. رنگ‌بندی کامنت‌ها =====
	Dim commentPattern As String = "<!--.*?-->"
	Dim commentMatcher As Matcher = Regex.Matcher(commentPattern, rs)
	Do While commentMatcher.Find
		txtcolor.Color(0xFF808080, commentMatcher.GetStart(0), commentMatcher.GetEnd(0)) ' خاکستری
	Loop

	' ===== 5. رنگ‌بندی CSS و JavaScript =====
	HighlightEmbeddedCode(rs, "<style.*?>", "</style>", 0xFFD33682) ' بنفش برای CSS
	HighlightJavaScriptCode(rs, "<script>", "</script>") ' تفکیک رنگ در جاوااسکریپت
	HighlightBody(rs, "<style>", "</style>") ' رنگ‌بندی `body {}` درون CSS
End Sub

Sub HighlightJavaScriptCode(rs As String, startTag As String, endTag As String)
	Dim startMatcher As Matcher = Regex.Matcher(startTag, rs)
	Dim endMatcher As Matcher = Regex.Matcher(endTag, rs)
    
	Do While startMatcher.Find And endMatcher.Find
		Dim codeStart As Int = startMatcher.GetEnd(0)
		Dim codeEnd As Int = endMatcher.GetStart(0)
		If codeStart < codeEnd Then
			Dim codeSnippet As String = rs.SubString2(codeStart, codeEnd)

			' 🌟 دسته‌بندی کلمات کلیدی
			Dim controlFlowKeywords As String = "\b(if|else|switch|case)\b" ' سبز
			Dim declarationKeywords As String = "\b(const|let|var|function)\b" ' آبی
			Dim specialKeywords As String = "\b(return|break|continue|new|typeof|catch|throw)\b" ' نارنجی

			' 🔹 رنگ‌بندی `if`, `else`, `switch`, `case` (سبز)
			Dim controlMatcher As Matcher = Regex.Matcher(controlFlowKeywords, codeSnippet)
			Do While controlMatcher.Find
				txtcolor.Color(0xFF008000, codeStart + controlMatcher.GetStart(0), codeStart + controlMatcher.GetEnd(0))
			Loop

			' 🔹 رنگ‌بندی `const`, `let`, `var`, `function` (آبی)
			Dim declarationMatcher As Matcher = Regex.Matcher(declarationKeywords, codeSnippet)
			Do While declarationMatcher.Find
				txtcolor.Color(0xFF0000FF, codeStart + declarationMatcher.GetStart(0), codeStart + declarationMatcher.GetEnd(0))
			Loop

			' 🔹 رنگ‌بندی `return`, `break`, `continue`, `new`, `typeof`, `catch`, `throw` (نارنجی)
			Dim specialMatcher As Matcher = Regex.Matcher(specialKeywords, codeSnippet)
			Do While specialMatcher.Find
				txtcolor.Color(0xFFFF4500, codeStart + specialMatcher.GetStart(0), codeStart + specialMatcher.GetEnd(0))
			Loop

			' ✨ رشته‌ها (Strings)
			Dim stringPattern As String = """(.*?)""|'(.*?)'"
			Dim stringMatcher As Matcher = Regex.Matcher(stringPattern, codeSnippet)
			Do While stringMatcher.Find
				txtcolor.Color(0xFFFF4500, codeStart + stringMatcher.GetStart(0), codeStart + stringMatcher.GetEnd(0)) ' نارنجی
			Loop

			' 🔢 اعداد (Numbers)
			Dim numberPattern As String = "\b\d+(\.\d+)?\b"
			Dim numberMatcher As Matcher = Regex.Matcher(numberPattern, codeSnippet)
			Do While numberMatcher.Find
				txtcolor.Color(0xFF6A5ACD, codeStart + numberMatcher.GetStart(0), codeStart + numberMatcher.GetEnd(0)) ' بنفش
			Loop

			' 🎯 پرانتزها (Parentheses)
			Dim parenthesesPattern As String = "[()]"
			Dim parenthesesMatcher As Matcher = Regex.Matcher(parenthesesPattern, codeSnippet)
			Do While parenthesesMatcher.Find
				txtcolor.Color(0xFFDAA520, codeStart + parenthesesMatcher.GetStart(0), codeStart + parenthesesMatcher.GetEnd(0)) ' طلایی
			Loop
			
			' ✨ رشته‌ها (Strings) با تفکیک کوتیشن و محتوای داخلی
			Dim stringPattern As String = """(.*?)""|'(.*?)'"
			Dim stringMatcher As Matcher = Regex.Matcher(stringPattern, codeSnippet)
			Do While stringMatcher.Find
				' رنگ کوتیشن‌ها (سبز)
				txtcolor.Color(0xFF025B00, codeStart + stringMatcher.GetStart(0), codeStart + stringMatcher.GetStart(0) + 1) ' اولین "
				txtcolor.Color(0xFF025B00, codeStart + stringMatcher.GetEnd(0) - 1, codeStart + stringMatcher.GetEnd(0)) ' آخرین "

				' رنگ متن داخل کوتیشن (سبز کم‌رنگ)
				txtcolor.Color(0xFF35AB00, codeStart + stringMatcher.GetStart(0) + 1, codeStart + stringMatcher.GetEnd(0) - 1)
			Loop

			' 🔹 رنگ‌بندی نام توابع (صورتی)
			Dim functionPattern As String = "\b[a-zA-Z_]\w*(?=\()"
			Dim functionMatcher As Matcher = Regex.Matcher(functionPattern, codeSnippet)
			Do While functionMatcher.Find
				txtcolor.Color(0xFF69B4, codeStart + functionMatcher.GetStart(0), codeStart + functionMatcher.GetEnd(0)) ' صورتی
			Loop

			' 🔹 رنگ‌بندی عملگرهای منطقی (قرمز)
			Dim logicalOperatorsPattern As String = "(&&|\|\||!)"
			Dim logicalOperatorsMatcher As Matcher = Regex.Matcher(logicalOperatorsPattern, codeSnippet)
			Do While logicalOperatorsMatcher.Find
				txtcolor.Color(0xFFFF0000, codeStart + logicalOperatorsMatcher.GetStart(0), codeStart + logicalOperatorsMatcher.GetEnd(0)) ' قرمز
			Loop

			' 🔹 رنگ‌بندی کامنت‌ها (خاکستری کم‌رنگ)
			Dim commentPattern As String = "//.*?$"
			Dim commentMatcher As Matcher = Regex.Matcher(commentPattern, codeSnippet)
			Do While commentMatcher.Find
				txtcolor.Color(0xFFA9A9A9, codeStart + commentMatcher.GetStart(0), codeStart + commentMatcher.GetEnd(0)) ' خاکستری کم‌رنگ
			Loop




		End If
	Loop
End Sub

Sub HighlightEmbeddedCode(rs As String, startTag As String, endTag As String, color As Int)
	Dim startMatcher As Matcher = Regex.Matcher(startTag, rs)
	Dim endMatcher As Matcher = Regex.Matcher(endTag, rs)
    
	Do While startMatcher.Find And endMatcher.Find
		Dim codeStart As Int = startMatcher.GetEnd(0)
		Dim codeEnd As Int = endMatcher.GetStart(0)
		If codeStart < codeEnd Then
			txtcolor.Color(color, codeStart, codeEnd)
		End If
	Loop
End Sub

Sub HighlightBody(rs As String, startTag As String, endTag As String)
	Dim startMatcher As Matcher = Regex.Matcher(startTag, rs)
	Dim endMatcher As Matcher = Regex.Matcher(endTag, rs)

	Do While startMatcher.Find And endMatcher.Find
		Dim codeStart As Int = startMatcher.GetEnd(0)
		Dim codeEnd As Int = endMatcher.GetStart(0)
		If codeStart < codeEnd Then
			Dim codeSnippet As String = rs.SubString2(codeStart, codeEnd)

			' 🔹 رنگ‌بندی `body {}` (بنفش)
			Dim bodyPattern As String = "\bbody\s*\{"
			Dim bodyMatcher As Matcher = Regex.Matcher(bodyPattern, codeSnippet)
			Do While bodyMatcher.Find
				txtcolor.Color(0xFFD33682, codeStart + bodyMatcher.GetStart(0), codeStart + bodyMatcher.GetEnd(0))
			Loop

			' ✨ ویژگی‌های CSS (آبی)
			Dim cssPropertiesPattern As String = "\b(font-family|margin|padding|text-align|direction|display|color|background|width|height)\b"
			Dim cssPropertiesMatcher As Matcher = Regex.Matcher(cssPropertiesPattern, codeSnippet)
			Do While cssPropertiesMatcher.Find
				txtcolor.Color(0xFF0000FF, codeStart + cssPropertiesMatcher.GetStart(0), codeStart + cssPropertiesMatcher.GetEnd(0))
			Loop

			' 🟢 مقدار ویژگی‌ها (سبز)
			Dim cssValuesPattern As String = ":.*?;"
			Dim cssValuesMatcher As Matcher = Regex.Matcher(cssValuesPattern, codeSnippet)
			Do While cssValuesMatcher.Find
				txtcolor.Color(0xFF008000, codeStart + cssValuesMatcher.GetStart(0) + 1, codeStart + cssValuesMatcher.GetEnd(0) - 1) ' سبز
			Loop

			' 🏷️ کامنت‌های CSS (خاکستری)
			Dim cssCommentsPattern As String = "/\*.*?\*/"
			Dim cssCommentsMatcher As Matcher = Regex.Matcher(cssCommentsPattern, codeSnippet)
			Do While cssCommentsMatcher.Find
				txtcolor.Color(0xFF808080, codeStart + cssCommentsMatcher.GetStart(0), codeStart + cssCommentsMatcher.GetEnd(0))
			Loop
		End If
	Loop
End Sub

Sub IsNumber1(s As String) As Boolean
	If s.Length = 0 Then Return False
	Dim dotFound As Boolean = False
	For i = 0 To s.Length - 1
		Dim c As String = s.CharAt(i)
		Dim charCode As Int = Asc(c)
		If charCode >= Asc("0") And charCode <= Asc("9") Then
			' رقم معتبر
		Else If c = "." And Not (dotFound) Then
			dotFound = True
		Else
			Return False
		End If
	Next
	Return True
End Sub

