Sub JobDone(job As HttpJob)
	If edite=False Then

		If job.Success  Then
			
			

        
			If job.JobName = "Job2" And job2.GetString<>"" And job2.GetString<>Null And job2.GetString<>jb2(bs) Then
				jb2(bs)=job2.GetString
				Dim jsonPars As JSONParser
				jsonPars.Initialize(job.GetString)
				
				Try
					Dim root As Map = jsonPars.NextObject
					Dim number As String = root.Get(serverkey(bs))

                       
					
					If vwordstart(bs).Length>0 Or vwordend(bs).Length>0 Then
				
					
						Extractval(number)
    
						number=valueword
					End If
					If sendb(bs) = "recive_server_text" And number<>"" And  number<>"null"    Then
						If IsNumber(kadres(bs)) Then
						If kadres(bs)>99 And kadres(bs)<1000  Then
							
							
							Dim len,adsk As Short
							len=number.Length
							Dim ads As String
							If len >2 Then
								ads=number.SubString(len-3)
							End If
						
							If IsNumber(ads) Then adsk=ads
							If adsk>99 And adsk<1000 Then
					
								If   adsk=kadres(bs) And edite=False Then
									Dim textt As String
									textt=number.SubString2(0,(len-3))
									If  knameshow(bs) = False Then k(bs).Text = textt & kname(bs)
									If knameshow(bs) = True Then k(bs).Text = textt

							
								End If
							End If
							
							End If
						Else
							If  knameshow(bs) = False Then k(bs).Text = number & kname(bs)
							If  knameshow(bs) = True Then k(bs).Text = number

						End If

			

					End If
					
					If sendb(bs) = "recive_server_val" And number<>"" And  number<>"null"   Then
						
						If number <> "" Then
							number=Round(number)
							If edite= False And inmsg = True Then msg1.Text = number
							If IsNumber(number) Then
								
								Dim yin As Int = number
								If yin > 0 And yin < 99999999 Then hh
							End If
						End If
					End If
					
					
					
					''''''''''''''''''''''''''''''''

					errr=0
				Catch
					errr=errr+1
			
					If errr=10   Then
						ToastMessageShow(xn(281), False)'"Error: Failed to parse JSON data."
						errr=0
					End If
					
				
				End Try
				
				

				
				
			End If
			err=0
			
		Else
	
			Dim ers As Byte
			ers= err Mod 10
			If ers=0   Then ToastMessageShow(xn(282) & job.ErrorMessage, False)'"Error server: "

		End If
		'job.Release
	
	End If
End Sub

Sub Extractval(input As String) As String
	Dim startIndex As Int = input.IndexOf(vwordstart(bs))
	If startIndex = -1 Then
		Return ""
	End If

	startIndex = startIndex + vwordstart(bs).Length
	Dim endIndex As Int = input.IndexOf2(vwordend(bs), startIndex)
	If endIndex = -1 Then
		Return ""
	End If
	Dim valueword As String = input.SubString2(startIndex, endIndex)
	Return valueword
End Sub

